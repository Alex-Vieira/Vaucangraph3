<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<AnchorPane fx:id="anchorPaneJanela" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vaucangraph.ControleDeJanela.ControleDeJanelaView">
    <children>
      <ToolBar fx:id="toolBarJanela" AnchorPane.rightAnchor="0.0">
        <items>
              <JFXButton fx:id="BTNminimizar" contentDisplay="GRAPHIC_ONLY">
                  <graphic>
                      <VBox>
                          <children>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                  <image>
                                      <Image url="@../icons/minimizar3.png" />
                                  </image>
                              </ImageView>
                          </children>
                      </VBox>
                  </graphic>
              </JFXButton>
                      
              <JFXButton fx:id="BTNmaximizar" contentDisplay="GRAPHIC_ONLY">
                  <graphic>
                      <VBox>
                          <children>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                  <image>
                                      <Image url="@../icons/maximizar.png" />
                                  </image>
                              </ImageView>
                          </children>
                      </VBox>
                  </graphic>
              </JFXButton>
                      
              <JFXButton fx:id="BTNfechar" contentDisplay="GRAPHIC_ONLY">
                  <graphic>
                      <VBox>
                          <children>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                  <image>
                                      <Image url="@../icons/fechar3.png" />
                                  </image>
                              </ImageView>
                          </children>
                      </VBox>
                  </graphic>
              </JFXButton>
        </items>
      </ToolBar>
                
    </children>
</AnchorPane>
